{
    "name": "nes/sistemalert",
    "description": "paquete para la generacion y gestion de alertas y mensajes push.",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Suarez",
            "email": "ti@nes360.org"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.5",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/session": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/view": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "SystemAlert\\": "vendor/system-alert/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "SystemAlert\\SystemAlertServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
